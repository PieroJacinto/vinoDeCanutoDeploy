<div class="container-fluid mt-5 ">
    <div class="row ">
        <div class="owl-carousel owl-theme ">
            <% if (productos.length > 0) { %>
                <% productos.forEach(element => { %>
                    <div class="item">                       
                        <div>
                            <a href="/detalleProducto/<%= element.id %>">
                                <img src="images/vinos/<%= element.imagen %>" alt="" srcset="" class="tarjeta-imagen">
                            </a>
                        </div>
                        <div class="contenido-tarjeta">                                
                            <a href="/detalleProducto/<%= element.id %>">
                                <p><%= element.nombre %></p>
                                <p>$<%= element.precio %></p>
                            </a>
                        </div>
                        <div class="boton-tarjeta-ultimos-ingresos">
                            <a href="/detalleProducto/<%= element.id %>">
                                Ver Producto
                            </a>
                        </div>                             
                    </div>                                                                                     
                    
                <% }) %>
            <% } %>
        </div>
    </div>
</div>           